<package>
  <description brief="rqt_srv">
	A Python GUI plugin for introspecting available ROS message types.
  </description>
  <author>Aaron Blasdel</author>
  <license>BSD</license>
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/rqt_srv</url>
  <depend package="rospy"/>
  <depend package="rqt_gui"/>
  <depend package="rqt_gui_py"/>
  <depend package="rqt_msg"/>
  <depend package="rosmsg"/>
  <export>
    <rqt_gui plugin="${prefix}/plugin.xml"/>
  </export>
</package>


