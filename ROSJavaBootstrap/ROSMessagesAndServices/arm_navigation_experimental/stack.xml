  <depend stack="octomap_msgs" /> <!-- octomap_mapping- -->
  <depend stack="octomap_ros" /> <!-- octomap_ros-->
  <depend stack="orocos_kinematics_dynamics" /> <!-- kdl -->
  <depend stack="perception_pcl" /> <!-- pcl_ros -->
