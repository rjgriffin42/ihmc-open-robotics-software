<package>
  <description brief="rqt_msg">
     A Python GUI plugin for introspecting available ROS message types.
  </description>
  <author>Aaron Blasdel</author>
  <license>BSD</license>
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/rqt_msg</url>
  <depend package="rospy"/>
  <depend package="rqt_gui"/>
  <depend package="rqt_gui_py"/>
  <depend package="rqt_py_common"/>
  <depend package="rqt_console"/>
  <depend package="roslib"/>
  <depend package="rosmsg"/>
  <export>
    <rqt_gui plugin="${prefix}/plugin.xml"/>
  </export>
</package>


