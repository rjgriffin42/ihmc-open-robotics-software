<stack>
  <description brief="rqt">
    Integration of the ROS package system and ROS-specific plugins for a Qt-based GUI.
  </description>
  <author>Maintained by Dirk Thomas</author>
  <license>BSD</license>
  <review status="API cleared" notes="2012-06-20"/>
  <url>http://ros.org/wiki/rqt</url>

  <depend stack="ros"/>
  <depend stack="common_msgs"/> <!-- geometry_msgs, sensor_msgs -->
  <depend stack="geometry"/> <!-- tf -->
  <depend stack="geometry_experimental"/> <!-- tf2 -->
  <depend stack="image_common"/> <!-- image_transport -->
  <depend stack="nodelet_core"/> <!-- nodelet -->
  <depend stack="qt_gui_core"/> <!-- qt_dotgraph, qt_gui_core, qt_gui_cpp -->
  <depend stack="ros_comm"/> <!-- roscpp, rosgraph, rosmsg, rosnode, rospy, rosservice, rostopic, rosgraph-->
  <depend stack="std_msgs"/> <!-- std_msgs -->
  <depend stack="vision_opencv"/> <!-- cv_bridge -->
  <depend stack="visualization"/> <!-- rviz -->
  <depend stack="pr2_power_drivers"/> <!-- rqt_pr2_dashboard -->
</stack>
