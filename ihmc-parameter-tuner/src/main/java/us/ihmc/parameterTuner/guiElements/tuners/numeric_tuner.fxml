<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<fx:root type="HBox" spacing="10.0" alignment="CENTER_LEFT"
	maxHeight="-Infinity" maxWidth="-Infinity" prefWidth="800"
	xmlns:fx="http://javafx.com/fxml">
	<padding><Insets top="0" right="5" bottom="5" left="5"/></padding>
	<children>
		<Text text="Value" />
		<StackPane fx:id="valuePane" prefWidth="120" />
		<Text text="Min" />
		<StackPane fx:id="minPane" prefWidth="120" />
		<StackPane fx:id="sliderPane" prefWidth="200"/>
		<Text text="Max" />
		<StackPane fx:id="maxPane" prefWidth="120" />
	</children>
</fx:root>