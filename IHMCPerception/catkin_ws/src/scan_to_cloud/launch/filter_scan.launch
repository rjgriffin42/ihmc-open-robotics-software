<launch>
  <node pkg="laser_filters" type="scan_to_cloud_filter_chain" name="lidar_scan_filter">
    <rosparam command="load" file="shadows.yaml" />
    <!--rosparam command="load" file="my_cloud_config.yaml" /-->
    <param name="high_fidelity" value="true" />
    <param name="target_frame" type="string" value="world" />
    <remap from="scan" to="/multisense/lidar_scan"/>
    <remap from="cloud_filtered" to="/multisense/filtered_cloud" />
  </node>
  <node pkg="laser_assembler" type="point_cloud2_assembler" name="point_cloud_assembler">
    <remap from="cloud" to="/multisense/filtered_cloud" />
    <param name="fixed_frame" value="world" />
    <param name="max_clouds" value="1200" />
  </node>
  <!--include file="$(find urdf_tutorial)/launch/display.launch" >
     <arg name="model" value="$(find atlas_description)/urdf/atlas_v4.urdf" />
  </include-->

</launch>
