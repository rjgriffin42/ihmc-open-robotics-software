<?xml version="1.0"?>
<package>
  <name>lidar_to_point_cloud_transformer</name>
  <version>0.1.0</version>
  <description>Transforms the data from a rotating lidar to a point cloud per scan and an assembled point cloud per rotation.</description>
  <author email="pfankhauser@ethz.ch">Peter Fankhauser</author>
  <maintainer email="pfankhauser@ethz.ch">Peter Fankhauser</maintainer>
  <license>TODO</license>
  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>cmake_modules</build_depend>
  <build_depend>roscpp</build_depend>
  <build_depend>laser_geometry</build_depend>
  <build_depend>sensor_msgs</build_depend>
  <build_depend>tf</build_depend>
  <build_depend>pcl_ros</build_depend>
  <build_depend>moveit_ros_perception</build_depend>
  <build_depend>geometric_shapes</build_depend>
  <build_depend>moveit_ros_planning</build_depend>
  <build_depend>tf_conversions</build_depend>

  <run_depend>roscpp</run_depend>
  <run_depend>laser_geometry</run_depend>
  <run_depend>sensor_msgs</run_depend>
  <run_depend>tf</run_depend>
  <run_depend>pcl_ros</run_depend>
  <run_depend>moveit_ros_perception</run_depend>
  <run_depend>geometric_shapes</run_depend>
  <run_depend>tf_conversions</run_depend>
 <run_depend>moveit_ros_planning</run_depend>
  
</package>
