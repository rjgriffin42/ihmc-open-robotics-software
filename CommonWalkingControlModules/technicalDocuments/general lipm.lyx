\begin_layout Standard
\begin_inset Formula 
\[
\ddot{r}=\underbrace{\left(\begin{array}{cc}
1 & 0\\
0 & 1\\
k_{x} & k_{y}
\end{array}\right)}_{A}\left(\begin{array}{c}
\ddot{x}\\
\ddot{y}
\end{array}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
mk_{x}\ddot{x}+mk_{y}\ddot{y}=-mg_{z}+f_{z}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
f & -mg=\left(\begin{array}{c}
m\ddot{x}\\
m\ddot{y}\\
m\left(k_{x}\ddot{x}+k_{y}\ddot{y}\right)
\end{array}\right)=\left(\begin{array}{c}
-m\omega_{0}^{2}p_{x}\\
-m\omega_{0}^{2}p_{y}\\
f_{z}
\end{array}\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\left(\begin{array}{c}
\ddot{x}\\
\ddot{y}
\end{array}\right)=-\omega_{0}^{2}\left(\begin{array}{c}
p_{x}\\
p_{y}
\end{array}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
f_{z}-mg_{z}=-mk_{x}\omega_{0}^{2}p_{x}-mk_{y}\omega_{0}^{2}p_{y}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
f=\left(\begin{array}{cc}
-m\omega_{0}^{2} & 0\\
0 & -m\omega_{0}^{2}\\
-mk_{x}\omega_{0}^{2} & -mk_{y}\omega_{0}^{2}
\end{array}\right)\left(\begin{array}{c}
p_{x}\\
p_{y}
\end{array}\right)+mg
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\dot{h}=\left(\begin{array}{c}
\left(\begin{array}{c}
f_{z}p_{y}-m\omega_{0}^{2}p_{y}p_{z}\\
-f_{z}p_{x}+m\omega_{0}^{2}p_{x}p_{z}\\
0
\end{array}\right)+\tau_{n}\\
\left(\begin{array}{c}
m\omega_{0}^{2}p_{x}\\
m\omega_{0}^{2}p_{y}\\
f_{z}-mg_{z}
\end{array}\right)
\end{array}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
