<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="us.ihmc.quadrupedFootstepPlanning.ui.controllers.PlannerReachParametersUIController">
    <children>
      <HBox spacing="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
                  <GridPane hgap="5.0" minWidth="-Infinity" vgap="5.0">
                      <columnConstraints>
                          <ColumnConstraints hgrow="SOMETIMES" />
                          <ColumnConstraints hgrow="SOMETIMES" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                          <ColumnConstraints hgrow="SOMETIMES" />
                          <ColumnConstraints hgrow="SOMETIMES" />
                          <ColumnConstraints hgrow="SOMETIMES" />
                          <ColumnConstraints hgrow="SOMETIMES" />
                      </columnConstraints>
                      <rowConstraints>
                  <RowConstraints vgrow="SOMETIMES" />
                          <RowConstraints vgrow="SOMETIMES" />
                          <RowConstraints vgrow="SOMETIMES" />
                          <RowConstraints vgrow="SOMETIMES" />
                          <RowConstraints />
                          <RowConstraints />
                      </rowConstraints>
                      <children>

                                <Label minWidth="-Infinity" text="Max front step reach" GridPane.halignment="RIGHT" GridPane.rowIndex="1" />
                          <Label minWidth="-Infinity" text="Max hind step reach" GridPane.columnIndex="2" GridPane.halignment="RIGHT" GridPane.rowIndex="1" />
                          <Label minWidth="-Infinity" text="Max step width" GridPane.columnIndex="2" GridPane.halignment="RIGHT" GridPane.rowIndex="2" />
                          <Label minWidth="-Infinity" text="Min step width" GridPane.halignment="RIGHT" GridPane.rowIndex="2" />
                          <Label minWidth="-Infinity" text="Max step yaw" GridPane.columnIndex="2" GridPane.halignment="RIGHT" GridPane.rowIndex="3" />
                          <Label minWidth="-Infinity" text="Min step yaw" GridPane.halignment="RIGHT" GridPane.rowIndex="3" />

                                <Label minWidth="-Infinity" text="Max front step length" GridPane.columnIndex="4" GridPane.halignment="RIGHT" GridPane.rowIndex="1" />
                          <Label minWidth="-Infinity" text="Min front step length" GridPane.columnIndex="4" GridPane.halignment="RIGHT" GridPane.rowIndex="2" />
                          <Label minWidth="-Infinity" text="Max hind step length" GridPane.columnIndex="4" GridPane.halignment="RIGHT" GridPane.rowIndex="3" />
                          <Label minWidth="-Infinity" text="Min hind step length" GridPane.columnIndex="4" GridPane.halignment="RIGHT" GridPane.rowIndex="4" />
                          <Label minWidth="-Infinity" text="Step Z" GridPane.columnIndex="4" GridPane.halignment="RIGHT" GridPane.rowIndex="5" />

                                <Label minWidth="-Infinity" text="Flat" GridPane.columnIndex="5" GridPane.halignment="CENTER" />
                          <Label minWidth="-Infinity" text="Up" GridPane.columnIndex="6" GridPane.halignment="CENTER" />
                          <Label minWidth="-Infinity" text="Down" GridPane.columnIndex="7" GridPane.halignment="CENTER" />

                                <Label minWidth="-Infinity" text="Max step Z" GridPane.columnIndex="2" GridPane.halignment="RIGHT" GridPane.rowIndex="4" />

      
                                <Spinner fx:id="maxFrontStepReach" editable="true" prefWidth="100.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                          <Spinner fx:id="maxHindStepReach" editable="true" prefWidth="100.0" GridPane.columnIndex="3" GridPane.rowIndex="1" />
                          <Spinner fx:id="maxStepWidth" editable="true" prefWidth="100.0" GridPane.columnIndex="3" GridPane.rowIndex="2" />
                          <Spinner fx:id="minStepWidth" editable="true" prefWidth="100.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                          <Spinner fx:id="maxStepYaw" editable="true" prefWidth="100.0" GridPane.columnIndex="3" GridPane.rowIndex="3" />
                          <Spinner fx:id="minStepYaw" editable="true" prefWidth="100.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />

                                <Spinner fx:id="maxFrontStepLength" editable="true" prefWidth="100.0" GridPane.columnIndex="5" GridPane.rowIndex="1" />
                          <Spinner fx:id="minFrontStepLength" editable="true" prefWidth="100.0" GridPane.columnIndex="5" GridPane.rowIndex="2" />
                          <Spinner fx:id="maxHindStepLength" editable="true" prefWidth="100.0" GridPane.columnIndex="5" GridPane.rowIndex="3" />
                          <Spinner fx:id="minHindStepLength" editable="true" prefWidth="100.0" GridPane.columnIndex="5" GridPane.rowIndex="4" />

                                <Spinner fx:id="maxFrontStepLengthWhenSteppingUp" editable="true" prefWidth="100.0" GridPane.columnIndex="6" GridPane.rowIndex="1" />
                          <Spinner fx:id="minFrontStepLengthWhenSteppingUp" editable="true" prefWidth="100.0" GridPane.columnIndex="6" GridPane.rowIndex="2" />
                          <Spinner fx:id="maxHindStepLengthWhenSteppingUp" editable="true" prefWidth="100.0" GridPane.columnIndex="6" GridPane.rowIndex="3" />
                          <Spinner fx:id="minHindStepLengthWhenSteppingUp" editable="true" prefWidth="100.0" GridPane.columnIndex="6" GridPane.rowIndex="4" />
                          <Spinner fx:id="stepZForSteppingUp" editable="true" prefWidth="100.0" GridPane.columnIndex="6" GridPane.rowIndex="5" />

                                <Spinner fx:id="maxFrontStepLengthWhenSteppingDown" editable="true" prefWidth="100.0" GridPane.columnIndex="7" GridPane.rowIndex="1" />
                          <Spinner fx:id="minFrontStepLengthWhenSteppingDown" editable="true" prefWidth="100.0" GridPane.columnIndex="7" GridPane.rowIndex="2" />
                          <Spinner fx:id="maxHindStepLengthWhenSteppingDown" editable="true" prefWidth="100.0" GridPane.columnIndex="7" GridPane.rowIndex="3" />
                          <Spinner fx:id="minHindStepLengthWhenSteppingDown" editable="true" prefWidth="100.0" GridPane.columnIndex="7" GridPane.rowIndex="4" />
                          <Spinner fx:id="stepZForSteppingDown" editable="true" prefWidth="100.0" GridPane.columnIndex="7" GridPane.rowIndex="5" />

                                <Spinner fx:id="maxStepChangeZ" editable="true" prefWidth="100.0" GridPane.columnIndex="3" GridPane.rowIndex="4" />

                            </children>

                        </GridPane>
                  <Button minWidth="-Infinity" onAction="#saveToFile" prefWidth="130.0" text="Save" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
    </children>
</AnchorPane>
